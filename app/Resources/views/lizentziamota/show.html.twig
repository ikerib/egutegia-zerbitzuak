{% extends 'base.html.twig' %}

{% block body %}
    <h1>Lizentziamotum</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ lizentziamotum.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ lizentziamotum.name }}</td>
            </tr>
            <tr>
                <th>Sinatubehar</th>
                <td>{% if lizentziamotum.sinatubehar %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{% if lizentziamotum.created %}{{ lizentziamotum.created|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{% if lizentziamotum.updated %}{{ lizentziamotum.updated|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_lizentziamota_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_lizentziamota_edit', { 'id': lizentziamotum.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
