{% extends 'base.html.twig' %}

{% block body %}
    <h1>Sinatzaileakdet</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sinatzaileakdet.id }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{% if sinatzaileakdet.created %}{{ sinatzaileakdet.created|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{% if sinatzaileakdet.updated %}{{ sinatzaileakdet.updated|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Orden</th>
                <td>{{ sinatzaileakdet.orden }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_sinatzaileakdet_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_sinatzaileakdet_edit', { 'id': sinatzaileakdet.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
