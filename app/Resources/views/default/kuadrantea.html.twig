        </div>
        <div class="tab-pane fade active in" role="tabpanel" id="abuztua" aria-labelledby="home-tab">
            {% include ':default:_tab_content.html.twig' with {'hilabetea': 'August', 'egunak': 31 } %}
        </div>
        <div class="tab-pane fade active in" role="tabpanel" id="iraila" aria-labelledby="home-tab">
            {% include ':default:_tab_content.html.twig' with {'hilabetea': 'September', 'egunak': 30 } %}
        </div>
        <div class="tab-pane fade active in" role="tabpanel" id="urria" aria-labelledby="home-tab">
            {% include ':default:_tab_content.html.twig' with {'hilabetea': 'October', 'egunak': 31 } %}
