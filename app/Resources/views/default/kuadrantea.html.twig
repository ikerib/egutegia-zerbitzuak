        <div class="tab-pane fade active in" role="tabpanel" id="azaroa" aria-labelledby="home-tab">
            {% include ':default:_tab_content.html.twig' with {'hilabetea': 'November', 'egunak': 30 } %}
        </div>
        <div class="tab-pane fade active in" role="tabpanel" id="abendua" aria-labelledby="home-tab">
            {% include ':default:_tab_content.html.twig' with {'hilabetea': 'December', 'egunak': 31 } %}
        </div>
        <div class="tab-pane fade active in" role="tabpanel" id="urtarrila2" aria-labelledby="home-tab">
            {% include ':default:_tab_content.html.twig' with {'hilabetea': 'January', 'egunak': 6, 'year': year+1} %}
        </div>
    </div>
{% endblock %}
